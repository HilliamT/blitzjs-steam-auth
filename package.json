{
  "name": "blitzjs-steam-auth",
  "version": "1.0.0",
  "scripts": {
    "dev": "blitz dev",
    "build": "blitz build",
    "start": "blitz start",
    "studio": "blitz prisma studio",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "prepare": "husky install"
  },
  "prisma": {
